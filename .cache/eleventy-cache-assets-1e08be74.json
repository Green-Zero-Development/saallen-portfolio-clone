[{"acf":{"icon":{"ID":258,"id":258,"title":"step-icon","filename":"step-icon.svg","filesize":6184,"url":"https://inside.saallen.com/wp-content/uploads/2021/08/step-icon.svg","link":"https://saallen.com/processes/step-1/step-icon/","alt":"","author":"1","description":"","caption":"","name":"step-icon","status":"inherit","uploaded_to":257,"date":"2021-08-27 15:57:07","modified":"2021-08-27 15:57:07","menu_order":0,"mime_type":"image/svg+xml","type":"image","subtype":"svg+xml","icon":"https://inside.saallen.com/wp-includes/images/media/default.png","width":68,"height":69,"sizes":{"thumbnail":"https://inside.saallen.com/wp-content/uploads/2021/08/step-icon.svg","thumbnail-width":150,"thumbnail-height":150,"medium":"https://inside.saallen.com/wp-content/uploads/2021/08/step-icon.svg","medium-width":300,"medium-height":300,"medium_large":"https://inside.saallen.com/wp-content/uploads/2021/08/step-icon.svg","medium_large-width":768,"medium_large-height":1,"large":"https://inside.saallen.com/wp-content/uploads/2021/08/step-icon.svg","large-width":1024,"large-height":1024,"1536x1536":"https://inside.saallen.com/wp-content/uploads/2021/08/step-icon.svg","1536x1536-width":68,"1536x1536-height":69,"2048x2048":"https://inside.saallen.com/wp-content/uploads/2021/08/step-icon.svg","2048x2048-width":68,"2048x2048-height":69}},"title":"Step 12","paragraph":"After the harvest is completed, we inspect the site before the harvester leaves to ensure that Georgia Best Management Practices for Forestry (BMPs) have been followed. At that point, the site is ready to be returned to the landowner to pursue their next steps, whether it be site-prep for reforestation, etc.","step_number":"12"}},{"acf":{"icon":{"ID":258,"id":258,"title":"step-icon","filename":"step-icon.svg","filesize":6184,"url":"https://inside.saallen.com/wp-content/uploads/2021/08/step-icon.svg","link":"https://saallen.com/processes/step-1/step-icon/","alt":"","author":"1","description":"","caption":"","name":"step-icon","sta